{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
    },
    "extends": 
        "eslint:recommended",

    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-var": "error",
        "camelcase": "error",
        "no-empty": "error",
        "default-case": "error",
        "eqeqeq": "error",
        "max-depth": ["error", 3]
    }
}
